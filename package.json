{
  "name": "OpenMetaOffice-Electron",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "ahooks": "^3.0.0-alpha.4",
    "antd": "^4.22.2",
    "axios": "^0.21.4",
    "cos-js-sdk-v5": "^1.3.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-store": "^8.0.1",
    "events": "^3.3.0",
    "express": "^4.17.3",
    "i18next": "^21.6.5",
    "i18next-browser-languagedetector": "^6.1.2",
    "md5": "^2.3.0",
    "react": "^17.0.2",
    "react-amap": "^1.2.8",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-i18next": "^11.15.3",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.4",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.0",
    "throttle-debounce": "^3.0.1",
    "typescript": "^4.1.2",
    "video.js": "^7.17.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start:renderer": "craco start",
    "build:renderer": "craco build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build:main": "craco build && tsc -p electron && electron-builder --macos --win",
    "test": "craco test",
    "eject": "react-scripts eject",
    "el": "electron .",
    "start:main": "run-p electron:*",
    "electron:run": "wait-on http://localhost:3000 && tsc -p electron && electron .",
    "electron:main": "wait-on http://localhost:3000 && tsc -p electron -w",
    "electron:renderer": "cross-env BROWSER=none npm run start:renderer"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@craco/craco": "^6.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.15",
    "@types/md5": "^2.3.1",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^17.0.0",
    "@types/react-scroll": "^1.8.3",
    "@types/throttle-debounce": "^2.1.0",
    "@types/video.js": "^7.3.31",
    "babel-plugin-import": "^1.13.3",
    "craco-less": "^1.20.0",
    "cross-env": "^7.0.3",
    "electron": "^15.3.0",
    "electron-builder": "^22.13.1",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "react-error-overlay": "6.0.9",
    "redux-devtools-extension": "^2.13.9",
    "source-map-explorer": "^2.5.2",
    "terser-webpack-plugin": "^4.2.3",
    "wait-on": "^6.0.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpackbar": "^5.0.0-3"
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  },
  "main": "build/electron/main.js",
  "homepage": ".",
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "productName": "OpenIM",
    "win": {
      "icon": "./build/icons/logo256x256.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "./build/icons/logo512x512.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./build/icons/logo16x16.ico",
      "uninstallerIcon": "./build/icons/logo16x16.ico",
      "installerHeaderIcon": "./build/icons/logo16x16.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "OpenIM"
    },
    "asar": false,
    "extraResources": {
      "from": "./electron/exec/",
      "to": "exec"
    }
  }
}
