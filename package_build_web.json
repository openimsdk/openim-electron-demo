{
  "name": "openim-pc-demo",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "OpenIM PC Demo.",
  "author": "blooming",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
